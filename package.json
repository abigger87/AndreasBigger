    "body-parser": "^1.18.3",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  },
  "proxy": "http://localhost:5000/",
  "description": "Personal website for Andreas Bigger Mark 2.0",
  "main": "server.js",
